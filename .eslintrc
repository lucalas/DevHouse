{
  "extends": "eslint-config-accurapp",
  "parser": "@typescript-eslint/parser",
  "env": {
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "react/react-in-jsx-scope": "off"
  },
  "globals": {
    "React": "writable"
  }
}
